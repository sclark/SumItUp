{
  "manifest_version": 2,
  "name": "SumItUp",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
	      "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Open SumItUp to get summarizing!"
  },
  "permissions" : [
    "tabs",
    "storage",
    "unlimitedStorage"
  ]
}
